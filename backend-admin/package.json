{
    "name": "uniapps-backend-admin",
    "version": "1.0.0",
    "description": "UniApps Admin Backend API Server",
    "main": "index.js",
    "scripts": {
        "dev": "nodemon --exec ts-node src/server.ts",
        "build": "tsc && shx cp -r src/routes/*.js dist/routes/ 2>/dev/null || true && shx cp -r src/middleware/*.js dist/middleware/ 2>/dev/null || true && shx cp -r src/services/*.js dist/services/ 2>/dev/null || true",
        "start": "node dist/server.js",
        "type-check": "tsc --noEmit",
        "db:generate": "prisma generate",
        "db:push": "prisma db push",
        "db:studio": "prisma studio",
        "db:migrate": "prisma migrate dev",
        "db:reset": "prisma migrate reset"
    },
