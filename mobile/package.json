{
  "name": "bondarys-mobile",
  "version": "1.0.0",
  "description": "Bondarys Family Management Mobile Application",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "dev": "expo start --dev-client",
    "prebuild": "ts-node ./scripts/validate-env.ts && ts-node ./scripts/branding-assets.ts",
    "validate:env": "ts-node ./scripts/validate-env.ts",
    "build": "npm run prebuild && expo build",
    "eject": "expo eject",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "i18n:validate": "ts-node ./scripts/i18n-tools.ts validate",
    "i18n:sync": "ts-node ./scripts/i18n-tools.ts sync",
    "i18n:sync:fill": "ts-node ./scripts/i18n-tools.ts sync --fill",
    "i18n:extract": "ts-node ./scripts/i18n-tools.ts extract",
    "i18n:add": "ts-node ./scripts/i18n-tools.ts add"
  },
  "dependencies": {
    "@expo-google-fonts/montserrat": "^0.2.3",
    "@expo/metro-config": "~0.18.11",
    "@expo/metro-runtime": "~3.2.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.29",
    "@reduxjs/toolkit": "^2.2.3",
    "@supabase/supabase-js": "^2.87.1",
    "axios": "^1.6.8",
    "expo": "~51.0.0",
    "expo-av": "~14.0.5",
    "expo-dev-client": "~4.0.14",
    "expo-document-picker": "~12.0.1",
    "expo-font": "~12.0.7",
    "expo-image-picker": "~15.0.5",
    "expo-linear-gradient": "~13.0.2",
    "expo-local-authentication": "~14.0.1",
    "expo-location": "~17.0.1",
    "expo-modules-core": "~1.12.18",
    "expo-notifications": "~0.28.1",
    "expo-secure-store": "~13.0.1",
    "i18next": "^23.16.8",
    "native-base": "^3.4.28",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-i18next": "^13.5.0",
    "react-native": "0.74.2",
    "react-native-calendars": "^1.1304.0",
    "react-native-draggable-flatlist": "^4.0.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-maps": "1.14.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "~3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.10",
    "react-native-webview": "13.8.6",
    "react-redux": "^9.1.1",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@playwright/test": "^1.57.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.45",
    "@types/react-native-vector-icons": "^6.4.18",
    "@types/react-test-renderer": "^19.1.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "https-browserify": "^1.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.12",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "react-test-renderer": "^18.2.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "url": "^0.11.4",
    "util": "^0.12.5"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "react": "$react",
    "react-dom": "$react",
    "react-native": "$react-native"
  }
}
