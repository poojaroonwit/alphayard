{
  "name": "bondarys-mobile",
  "version": "1.0.0",
  "description": "Bondarys Family Management Mobile Application",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "expo start --dev-client",
    "prebuild": "ts-node ./scripts/validate-env.ts && ts-node ./scripts/branding-assets.ts",
    "validate:env": "ts-node ./scripts/validate-env.ts",
    "build": "npm run prebuild && expo build",
    "eject": "expo eject",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "i18n:validate": "ts-node ./scripts/i18n-tools.ts validate",
    "i18n:sync": "ts-node ./scripts/i18n-tools.ts sync",
    "i18n:sync:fill": "ts-node ./scripts/i18n-tools.ts sync --fill",
    "i18n:extract": "ts-node ./scripts/i18n-tools.ts extract",
    "i18n:add": "ts-node ./scripts/i18n-tools.ts add"
  },
  "dependencies": {
    "@expo-google-fonts/montserrat": "^0.4.2",
    "@expo/metro-config": "~54.0.2",
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/cli-server-api": "^18.0.0",
    "@react-native-google-signin/google-signin": "^10.1.1",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^1.9.7",
    "@supabase/supabase-js": "^2.53.0",
    "axios": "^1.11.0",
    "expo": "~54.0.0",
    "expo-av": "^16.0.7",
    "expo-dev-client": "~6.0.18",
    "expo-document-picker": "^14.0.7",
    "expo-font": "~14.0.9",
    "expo-image-picker": "^17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-local-authentication": "~17.0.7",
    "expo-location": "~19.0.7",
    "expo-modules-core": "~3.0.26",
    "expo-notifications": "~0.32.13",
    "expo-secure-store": "~15.0.7",
    "i18next": "^23.16.8",
    "native-base": "^3.4.28",
    "normalize-css-color": "^1.0.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^13.5.0",
    "react-native": "0.81.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~18.2.45",
    "@types/react-native-vector-icons": "^6.4.18",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "assert": "^2.1.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "https-browserify": "^1.0.0",
    "jest-expo": "^54.0.12",
    "os-browserify": "^0.3.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.2.5",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^5.3.3",
    "url": "^0.11.4",
    "util": "^0.12.5"
  },
  "engines": {
    "node": ">=18"
  }
}
